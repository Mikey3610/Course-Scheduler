<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.C196_Application_Latest"
        tools:targetApi="31">
        <receiver
            android:name=".Receiver.MyReceiver"
            android:enabled="true"
            android:exported="true"></receiver>

        <activity
            android:name=".UI.AddEditAssessments"
            android:exported="false"
            android:label="Add/Edit Assessments"
            android:parentActivityName=".UI.EditCourses_AssessmentsList">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".UI.AddCourses"
            android:exported="false"
            android:label="Add Courses"
            android:parentActivityName=".UI.EditTerms_CoursesList">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".UI.AddTerms"
            android:exported="false"
            android:label="Add Terms"
            android:parentActivityName=".UI.AllTerms">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".UI.EditCourses_AssessmentsList"
            android:exported="false"
            android:label="Edit Courses and Assessments List"
            android:parentActivityName=".UI.EditTerms_CoursesList">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".UI.EditTerms_CoursesList"
            android:exported="false"
            android:label="Edit Terms and Courses List"
            android:parentActivityName=".UI.AllTerms">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".UI.AllTerms"
            android:exported="false"
            android:label="All Terms"
            android:parentActivityName=".UI.MainActivity">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".UI.MainActivity"
            android:exported="true"
            android:label="Course Scheduler">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
    </application>

</manifest>